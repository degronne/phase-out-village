    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "ol": "^10.6.1",
